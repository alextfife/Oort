#include<iostream>
using namespace std;
int main() 
{
var gumballs = rand()%1000+1;
var user = 0;
var start = 1;

while (start = 1) {
cout<<"Guess how many gumballs are in the machine?"<<endl;
cin >> user;



if (user > gumballs) {
cout<<"Too High"<<endl;
cin >>user;
}

if (user < gumballs) {
cout<< "Too low"<endl;
cin >> user;
}

if (user = gumballs) {
cout<<"Correct"<endl;
start = 0
}


}


















}
